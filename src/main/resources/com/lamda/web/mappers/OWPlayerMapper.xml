<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lamda.web.mappers.OWPlayerMapper">
    <select id="selectAll" resultType="com.lamda.web.music.OWPlayerDTO">
        select
            playerimg,
            player,
            name,
            hometown,
            teamimg,
            team,
            role
        from owplayer
    </select>
</mapper>
